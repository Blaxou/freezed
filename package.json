{
	"name": "freezed",
	"displayName": "Freezed",
	"description": "Generate Freezed classes in Flutter",
	"version": "0.3.0",
	"repository": "https://github.com/Blaxou/freezed",
	"publisher": "blaxou",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:freezed.generate",
		"onCommand:freezed.activate_build_runner"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "freezed.generate",
				"title": "Freezed : Generate a new Freezed class"
			},
			{
				"command": "freezed.activate_build_runner",
				"title": "Freezed : Build with build_runner"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "freezed.generate",
					"group": "1_modification"
				}
			]
		},
		"keybindings": [
			{
				"command": "freezed.activate_build_runner",
				"key": "Ctrl+Shift+B",
				"mac": "Cmd+Shift+B"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.15",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.9.7",
		"vscode-test": "^1.3.0"
	}
}